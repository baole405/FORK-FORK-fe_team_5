  "scripts": {
    "build-all-skip-cache": "nx run-many -t build --skip-nx-cache",
    "format:write": "nx format:write",
    "prepare": "node -e \"process.env.CI || process.env.DOCKER || process.exit(1)\" || (husky && git config --local include.path .gitconfig && pnpm run setup-nx-powerpack)",
