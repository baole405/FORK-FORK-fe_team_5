{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "outDir": "dist/cypress",
    "module": "nodenext",
    "moduleResolution": "nodenext",
    "target": "es5",
    "lib": ["es5", "dom"],
    "types": ["cypress", "node", "@testing-library/dom", "@testing-library/react"],
    "sourceMap": true
  },
  "include": [
    "cypress.config.mjs",
    "cypress.config.js",
    "**/*.ts",
    "**/*.js",
    "**/*.cy.ts",
    "**/*.cy.tsx",
    "**/*.cy.js",
    "**/*.cy.jsx",
    "**/*.d.ts",
    "vite.config.js",
    "vite.config.js"
  ],
  "exclude": ["test-output", "eslint.config.js", "eslint.config.cjs", "eslint.config.mjs"],
  "references": []
}
